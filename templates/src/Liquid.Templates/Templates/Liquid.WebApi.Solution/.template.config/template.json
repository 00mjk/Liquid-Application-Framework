{
  "$schema": "http://json.schemastore.org/template",
  "author": "Avanade Brazil",
  "classifications": [ "LAF", "WebApi", "Solution" ],
  "identity": "Liquid.WebApi.Solution",
  "name": "Liquid WebAPI solution (Domain and WebAPI projects.)",
  "shortName": "liquidwebapisolution",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "projectName": {
      "type": "parameter",
      "replaces": "PROJECTNAME",
      "FileRename": "PROJECTNAME",
      "defaultValue": "MyProject"
    },
    "entityName": {
      "type": "parameter",
      "replaces": "ENTITYNAME",
      "FileRename": "ENTITYNAME",
      "defaultValue": "MyObject"
    },
    "command": {
      "type": "parameter",
      "replaces": "COMMANDNAME",
      "FileRename": "COMMANDNAME",
      "defaultValue": "MyCommand"
    },
    "entityIdType": {
      "type": "parameter",
      "replaces": "ENTITYIDTYPE",
      "defaultValue": "int"
    }
  }
}