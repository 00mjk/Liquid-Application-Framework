{
  "$schema": "http://json.schemastore.org/template",
  "author": "Avanade Brazil",
  "classifications": [ "LAF", "Library" ],
  "identity": "Liquid.Domain.AddHandler",
  "name": "Liquid Add Domain Handler",
  "description": "Add a mediator pattern hanlder using Liquid Framework to a domain project.",
  "shortName": "liquiddomainaddhandler",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "projectName": {
      "type": "parameter",
      "replaces": "PROJECTNAME",
      "FileRename": "PROJECTNAME",
      "defaultValue": "MyProject"
    },
    "entityName": {
      "type": "parameter",
      "replaces": "ENTITYNAME",
      "FileRename": "ENTITYNAME",
      "defaultValue": "MyObject"
    },
    "command": {
      "type": "parameter",
      "replaces": "COMMANDNAME",
      "FileRename": "COMMANDNAME",
      "defaultValue": "MyCommand"
    },
    "entityIdType": {
      "type": "parameter",
      "replaces": "ENTITYIDTYPE",
      "defaultValue": "int"
    }
  }
}